interface Piece{

    int addr = 0;
    int size= 0;
    byte[] data= "abc".getBytes();

    ArrayList<String> communicationCard = null;
     public byte[] getMem(int addr, int size);
     public void setMem(byte[] data, int addr);
}
class RAM implements Piece {

    public byte[] getMem(int addr, int size) {
        return new byte[0];}
    public void setMem(byte[] data, int addr) {}


}

class Ethernet implements Piece{
    public byte[] getMem(int addr, int size) {
        return new byte[0];
    }

    public void setMem(byte[] data, int addr) {

    }
}

class TokenRing implements Piece{
    public byte[] getMem(int addr, int size) {
        return new byte[0];
    }

    public void setMem(byte[] data, int addr) {

    }



}



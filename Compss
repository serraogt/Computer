interface CommunicationCard{

    byte[] data= "abc".getBytes();
     public byte[] getCom(int size);
     public void setCom(byte[] b);
}
class RAM {

    public byte[] getMem(int addr, int size) {
        return new byte[0];}
    public void setMem(byte[] data, int addr) {}


}

class Ethernet implements CommunicationCard{
    public byte[] getCom(int size) {
        return new byte[0];
    }

    public void setCom(byte[] b) {

    }
}

class TokenRing implements CommunicationCard{
    public byte[] getCom( int size) {
        return new byte[0];
    }

    public void setCom(byte[] b) {

    }



}

